
  <h3>Recent changes</h3>
   <% @movies.each do |movie| %>
    <div class="post">
      <h2 class="title">
       <%= link_to movie.title, :controller => :movies, :action => :show, :id => movie.id %>
      </h2>
      <div class="entry">
        <h3>Watched: <%= movie.watched %></h3>
      </div>
    </div>
   <% end %>
